{
  "name": "dnd-monster-api",
  "version": "1.0.0",
  "description": "An api that serves up Dungeon's & Dragons monsters via GraphQL and contains a gRPC server too",
  "main": "src/main.js",
  "repository": "git@github.com:theoperatore/dnd-monster-api.git",
  "author": "Al Petersen <theoperatore@gmail.com>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=10.14.1",
    "npm": ">=6.4.1",
    "yarn": ">=1.12.3"
  },
  "scripts": {
    "build-db": "node src/db/createDb.js",
    "start": "node src/dnd-monster-api.js",
    "test": "jest",
    "start:grpc": "node src/grpc/server.js",
    "start:alorg": "node src/alorg/server.js",
    "forever": "forever -d -a -o out.log -e err.log start src/dnd-monster-api.js"
  },
  "dependencies": {
    "@grpc/proto-loader": "^0.3.0",
    "@theoperatore/alorg-service": "^1.0.2",
    "chalk": "^2.3.1",
    "cheerio": "^1.0.0-rc.2",
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "del": "^3.0.0",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-graphql": "^0.7.1",
    "fast-memoize": "^2.5.1",
    "fs-extra": "^7.0.1",
    "get-port": "^4.0.0",
    "google-protobuf": "^3.6.1",
    "graphql": "^14.0.2",
    "grpc": "^1.16.1",
    "helmet": "^3.15.0",
    "jsdom": "^13.0.0",
    "markdown-to-json": "^0.5.1",
    "marked": "^0.5.2",
    "request": "^2.88.0",
    "simple-git": "^1.107.0"
  },
  "devDependencies": {
    "jest": "^23.6.0"
  }
}
